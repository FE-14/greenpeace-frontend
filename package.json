{
  "name": "greenpeace",
  "version": "0.1.0",
  "private": true,
  "description": "Greenpeace adalah sebuah organisasi non-profit yang bergerak di bidang lingkungan hidup dan hak asasi manusia. Kami tim FE-14 mendapatkan Challenge Partner dari Greenpeace untuk membuat sebuah website yang dapat membantu mereka menyelesaikan masalah dan isu permasalahan lainnya.",
  "main": "index.js",
  "scripts": {
    "dev": "npx tailwindcss -i ./src/assets/css/index.css -o ./src/assets/css/output.css --watch",
    "build": "npx prettier --write . && npx tailwindcss -i ./src/assets/css/index.css -o ./src/assets/css/output.css --minify",
    "format": "npx prettier --write ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FE-14/greenpeace-frontend"
  },
  "keywords": [
    "greenpeace"
  ],
  "author": "FE-14",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/FE-14/greenpeace-frontend/issues"
  },
  "homepage": "https://github.com/FE-14/greenpeace-frontend#readme",
  "devDependencies": {
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "tailwindcss": "^3.1.8"
  }
}
